<!--pages/profile/profile.wxml-->
<view class='pro'> 
    <view class='form'>
      <input type='text' placeholder='用户名' bindinput='uname' value='{{value}}'/>
    </view>
    <view class='form'>
      <input type='password' placeholder='密码' bindinput='upwd' value='{{value}}'/>
    </view>
    <button bindtap='btn'>登录</button>
    <view class='flex'>
      <text class='ddd'>立即注册</text>
      <text class='pwd'>忘记密码？</text>
    </view>
</view>
